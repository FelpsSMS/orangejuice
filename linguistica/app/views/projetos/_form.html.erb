<%= simple_form_for(@projeto) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :projeto_titulo %>

    <%= f.input :projeto_descricao %>

    <%= f.association :pesquisador %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

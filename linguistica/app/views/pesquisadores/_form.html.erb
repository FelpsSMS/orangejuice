<%= simple_form_for(@pesquisador, :html => { :multipart => true }) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :pesquisador_nome %>
    <%= f.input :pesquisador_cnpq %>
    <%= f.input :pesquisador_contato %>
	<%= f.file_field :foto %>
	 


	 
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
